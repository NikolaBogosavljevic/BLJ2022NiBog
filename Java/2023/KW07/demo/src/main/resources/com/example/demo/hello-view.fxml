<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="446.0" prefWidth="533.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Pane prefHeight="386.0" prefWidth="552.0">
      <children>
      
          <Label fx:id="welcomeText" layoutX="168.0" layoutY="14.0" text="Personen Manager">
            <font>
               <Font size="19.0" />
            </font>
         </Label>
         <Text layoutX="120.0" layoutY="114.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:" />
         <TextField fx:id="nameField" layoutX="201.0" layoutY="97.0" prefHeight="25.0" prefWidth="178.0" promptText="Bog" />
         <Text layoutX="120.0" layoutY="158.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Vorname:" />
         <TextField fx:id="firstNameField" layoutX="201.0" layoutY="141.0" prefHeight="25.0" prefWidth="178.0" promptText="Tog" />
         <Text layoutX="120.0" layoutY="202.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Age:" />
         <TextField fx:id="ageField" layoutX="201.0" layoutY="185.0" prefHeight="25.0" prefWidth="178.0" promptText="4" />
         <Text layoutX="120.0" layoutY="243.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Favorite Food:" />
         <TextField fx:id="favFoodField" layoutX="201.0" layoutY="226.0" prefHeight="25.0" prefWidth="178.0" promptText="food" />
         <Label fx:id="showName" layoutX="233.0" layoutY="262.0" />
         <Label fx:id="showFirstName" layoutX="233.0" layoutY="279.0" />
         <Label fx:id="showFavFood" layoutX="233.0" layoutY="313.0" />
         <Label fx:id="showAge" layoutX="233.0" layoutY="296.0" />
         <ButtonBar layoutX="29.0" layoutY="332.0" prefHeight="40.0" prefWidth="200.0">
           <buttons>
             <Button fx:id="prevButton" mnemonicParsing="false" onAction="#onPrevButtonClick" text="&lt;" />
               <Button fx:id="newButton" mnemonicParsing="false" onAction="#onNewButtonClick" text="New" />
               <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onSaveButtonClick" text="Save" />
               <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Delete" />
               <Button fx:id="nextButton" mnemonicParsing="false" onAction="#onNextButtonClick" text="&gt;" />
           </buttons>
         </ButtonBar>
      </children>
   </Pane>
</VBox>
